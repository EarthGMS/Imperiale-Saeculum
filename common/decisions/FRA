FRA_developp_railway_colonial_organisation = {                                                   
			allowed = { tag = FRA }               
			available = {
			has_equipment = { train_equipment > 100 }
			num_of_civilian_factories_available_for_projects > 5
			}

			visible = {
				AND = {
					original_tag = FRA
					has_idea = FRA_Developped_infrastructure_africa_idea_1
				}
				AND = {
					original_tag = FRA
					has_idea = FRA_Developped_infrastructure_africa_idea_2
				}
				AND = {
					original_tag = FRA
					has_idea = FRA_Developped_infrastructure_africa_idea_3
				}
				AND = {
					original_tag = FRA
					has_idea = FRA_Developped_infrastructure_africa_idea_4
				}
			}
		
			fire_only_once = yes

			cost = 25
			
			days_remove = 0

			modifier = {
			civilian_factory_use = 3
			production_speed_industrial_complex_factor = -0.5
			production_speed_arms_factory_factor = -0.5
			production_speed_rail_way_factor = 0.1
			}
			
			remove_effect = {  
			custom_effect_tooltip = FRA_developped_north_african_railway_issue_effect 
			add_political_power = 50
			add_stability = 0.01
			add_ideas = FRA_Developped_infrastructure_africa_idea_5
			hidden_effect = {
			remove_ideas = {
				FRA_Developped_infrastructure_africa_idea_1
				}
			remove_ideas = {
				FRA_Developped_infrastructure_africa_idea_2
				}
			remove_ideas = {
				FRA_Developped_infrastructure_africa_idea_3
				}
			remove_ideas = {
				FRA_Developped_infrastructure_africa_idea_4
					}
				}
			} 
		}
	}

FRA_civilize_africa_stage_one_category = {

FRA_developped_north_africa = {                                                   
			allowed = { tag = FRA }               
			available = {
				custom_trigger_tooltip = {
					tooltip = FRA_developped_north_africa_issue_trigger		
			has_railway_connection = {
				start_province = 1145
				target_province = 12051
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 12857
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 7132
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 7081
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 7069
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 4108
			}
			has_railway_connection = {
				start_province = 1145
				target_province = 12034
			}
			
			459= {
					industrial_complex > 1
					air_base > 0
				}
			460 = {
					industrial_complex > 1
					air_base > 0
				}
			461= {
					industrial_complex > 1
					air_base > 0
				}
			462= {
					industrial_complex > 1
					air_base > 0
					}
				}
			}
			activation = {                         
                has_completed_focus = FRA_focus_on_africa                    
			}
			is_good = no 
			selectable_mission = no       
			days_mission_timeout = 720            
			timeout_effect = {                      
                add_political_power = -50
				add_stability = -0.025
			}
			complete_effect = {  
			custom_effect_tooltip = FRA_developped_north_africa_issue_effect 
			add_political_power = 25
			add_stability = 0.025
			add_timed_idea = {
				idea = FRA_Developped_North_Africa_idea
				days = 1800
			} 
		}
	}

FRA_distribute_tractor_to_the_farmer_constantine = {                                                   
	icon = generic_construction
	
	highlight_states = {
			highlight_state_targets = {
				state = 460
			}
		}
		
	on_map_mode = map_only	
		
	available = {
			owns_state = 460
			has_equipment = {
				motorized_equipment > 299
			}
			460 = { 
				NOT = {
					industrial_complex > 2
				}
			}
		}
	fire_only_once = no
	
	visible = {
		FRA = {
			has_completed_focus = FRA_focus_on_agriculture
			}
		460 = { 
				NOT = {
					industrial_complex > 2
				}
			}
		}
	cost = 5
	
	days_remove = 120
       remove_effect = {
			460 = {
				if = {
					limit = { is_controlled_by = FRA }
						add_extra_state_shared_building_slots = 1
						add_manpower = 5000
						add_equipment_to_stockpile = {
				type = motorized_equipment
				amount = -300
				producer = FRA
					}
				}
			}
		}
	}
FRA_develop_constantine_metalurgical_production = { 

		icon = steel

		allowed = {

		}

		highlight_states = {
			highlight_state_targets = {
				state = 460
			}
		}
		
		on_map_mode = map_only
		
		available = {
			num_of_civilian_factories_available_for_projects > 3
			owns_state = 460
			controls_state = 460
		}

		visible = {
			owns_state = 460
			controls_state = 460
			FRA = {
			has_completed_focus = FRA_focus_on_africa
			}
		}

		fire_only_once = yes

		cost = 15
		days_remove = 90

		modifier = {
			civilian_factory_use = 4
		}

		ai_will_do = {
			base = 0.3
	
			modifier = {  
				factor = 0.5
					has_political_power < 15
			}
			modifier = { 
				factor = 3
				has_political_power > 100
			}
			modifier = { 
				factor = 5
				has_available_resources_in_country = {
					resource = steel
					amount < 1
				}
			}
		}
		remove_effect = {
			460 = {
				add_resource = {
					type = steel
					amount = 7
				}
			}
		}
	}	
