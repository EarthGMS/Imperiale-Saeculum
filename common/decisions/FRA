FRA_developp_colonies_category = {

FRA_developped_administration_in_american_colonies = {                                                   
			allowed = { tag = FRA }               
			available = {
				custom_trigger_tooltip = {
					tooltip = FRA_developp_america_finale_stage_issue_effect		
				310 = {
					compliance > 70 
				}
				371 = {
					compliance > 70 
					}
				694 = {
					compliance > 70 
					}
				} 
				has_completed_focus = FRA_new_ameriga
			}
			activation = {                         
               has_completed_focus = FRA_extend_colonial_administration                    
			}
			is_good = no 
			selectable_mission = no       
			days_mission_timeout = 1800             
			timeout_effect = {                      
                add_political_power = -200
				add_stability = -0.1
			}
			complete_effect = {  
			add_political_power = 120
			add_stability = 0.05    
			add_ideas = { FRA_American_Jewel }
		}
	}

FRA_developped_trade_with_american_colonies = {                                                   
			allowed = { tag = FRA }               
			available = {
				custom_trigger_tooltip = {
					tooltip = FRA_developp_america_finale_stage_issue_effect		
				310= {
				
				}
				371 = {
					limit = { is_controlled_by = FRA }
					state_modifier = {
						modifier = {
							local_resources = 0.15
							}
						}
					add_building_construction = {
						type = naval_base
						level = 1
						province = 7552
						instant_build = yes
						}
					add_building_construction = {
						type = naval_base
						level = 1
						province = 4507
						instant_build = yes
					}
				}
				694= {
					industrial_complex > 1
					air_base > 0
				} 
				has_completed_focus = FRA_new_ameriga
			}
			activation = {                         
               has_completed_focus = FRA_developp_colonial_trade                    
			}
			is_good = no 
			selectable_mission = no       
			days_mission_timeout = 1800             
			timeout_effect = {                      
                add_political_power = -200
				add_stability = -0.1
			}
			complete_effect = {  
