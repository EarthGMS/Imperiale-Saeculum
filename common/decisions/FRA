			allowed = { tag = FRA }               
			available = {
				custom_trigger_tooltip = {
					tooltip = FRA_developp_america_finale_stage_issue_effect		
				310 = {
					compliance > 70 
				}
				371 = {
					compliance > 70 
					}
				694 = {
					compliance > 70 
					}
				} 
				AND = {
			has_completed_focus = FRA_developp_new_crop
			has_completed_focus = FRA_extend_labor_laws_in_america
			has_completed_focus = FRA_subsidize_strategic_sector_of_colonial_companies
				}
			}
			activation = {                         
                OR = {
			has_completed_focus = FRA_focus_on_agrarian_policies_in_america
			has_completed_focus = FRA_focus_on_transport_of_industrial_capabilities_in_america
				}                   
			}
			is_good = no 
			selectable_mission = no       
			days_mission_timeout = 1800             
			timeout_effect = {                      
                add_political_power = -200
				add_stability = -0.1
			}
			complete_effect = {  
			add_political_power = 120
			add_stability = 0.05    
			add_ideas = { FRA_American_Jewel }
		}
	}
	
FRA_develop_guiana_metalurgical_production = { 

		icon = steel

		allowed = {

		}

		highlight_states = {
			highlight_state_targets = {
				state = 310
			}
		}
		
		on_map_mode = map_only

		available = {
			num_of_civilian_factories_available_for_projects > 3
			owns_state = 310
			controls_state = 310
		}

		visible = {
			owns_state = 310
			controls_state = 310
			FRA = {
			has_completed_focus = FRA_focus_on_america
			}
		}

		fire_only_once = yes

		cost = 15
		days_remove = 90

		modifier = {
			civilian_factory_use = 4
		}

		ai_will_do = {
			base = 0.3
	
			modifier = {  
				factor = 0.5
					has_political_power < 30
			}
			modifier = { 
				factor = 3
				has_political_power > 100
			}
			modifier = { 
				factor = 5
				has_available_resources_in_country = {
					resource = steel
					amount < 1
				}
			}
		}
		remove_effect = {
			310 = {
				add_resource = {
					type = steel
					amount = 7
				}
			}
		}
	}
	
