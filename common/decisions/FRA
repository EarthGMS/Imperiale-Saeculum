	highlight_states = {
			highlight_state_targets = {
				state = 459
			}
		}
		
	on_map_mode = map_only	
		
	available = {
			owns_state = 459
			has_equipment = {
				infantry_equipment > 199
			}
			459 = { 
				NOT = {
					industrial_complex > 2
				}
			}
		}
	fire_only_once = yes
	
	visible = {
		FRA = {
			has_completed_focus = FRA_focus_on_colonial_labor
			}
		459 = { 
				NOT = {
					industrial_complex > 2
				}
			}
		}
		
	cost = 0

	custom_cost_trigger = { 
		has_manpower > 999 
	}
	
	modifier = {
			weekly_manpower = -100
		}
		
	days_remove = 70
        remove_effect = {
			459 = {
				if = {
					limit = { is_controlled_by = FRA }
						add_state_modifier = {
							modifier = {
								compliance_growth = 0.05
								}
							}
						}
					}
			add_equipment_to_stockpile = {
				type = infantry_equipment
				amount = -200
				producer = FRA
			}
		}
	}

