#############################
FRA_WAR_CATEGORY = {

FRA_War_USA_offensive_first_step = {                                                   
	allowed = { tag = FRA }               
			available = {
				custom_trigger_tooltip = {
					tooltip = FRA_first_war_with_false_usa_issue_trigger
				AND = {
					FRA = {
					controls_province = 12384
					}    
				}
				AND = {
					FRA = {
					controls_province = 12489
					}    
				}
				AND = {
					FRA = {
					controls_province = 12735
						}
					}    
				}
			}
			activation = {                         
                FRA = {
					has_offensive_war_with = USA
				}                    
			}
			is_good = no 
			selectable_mission = no       
			days_mission_timeout = 150            
			timeout_effect = {                      
                add_political_power = -150
				add_stability = -0.075
			}
			complete_effect = {  
			custom_effect_tooltip = FRA_first_war_with_false_usa_issue_effect 
			add_political_power = 100
			add_stability = 0.05
		}
	}
}

FRA_transcolonial_railway_system_category = {

FRA_developp_north_african_railway = {                                                   
			allowed = { tag = FRA }               
			available = {
			has_equipment = { train_equipment > 20 }
			num_of_civilian_factories_available_for_projects > 2
			}
			activation = {                         
                has_completed_focus = FRA_focus_on_africa                    
			}    
			fire_only_once = yes

			cost = 25
			
			days_remove = 0

			modifier = {
			civilian_factory_use = 3
			production_speed_industrial_complex_factor = -0.2
			production_speed_arms_factory_factor = -0.2
			production_speed_rail_way_factor = 0.5
			}
			
			remove_effect = {  
			custom_effect_tooltip = FRA_developped_north_african_railway_issue_effect 
			add_political_power = 15
			add_stability = 0.005 
			add_timed_idea = {
				idea = FRA_Developped_infrastructure_africa_idea_1
				days = 900
			} 
		}
	}
	
